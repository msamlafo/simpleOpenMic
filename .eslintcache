LETE\", \n                headers: new Headers({\n                    'Content-Type': 'application/json',\n                    Authorization: process.env.REACT_APP_user_token,\n                    //'Authorization': localStorage.getItem(\"token\")\n                })\n            })\n            .then(result => result.json())\n            .then(response => {\n                console.log(response);\n                if (response.status === 200){\n                    return <h5>Poem successfully deleted</h5>\n                }\n            })\n            .catch(err => console.log(err));\n        }\n        poemToDelete(); \n    }\n\n    render() { \n        return ( \n        <Button color=\"danger\" onClick={(p)=>this.handleDelete(this.props.poem)} >\n            Delete Poem\n        </Button>\n        );\n    }\n}\n \nexport default DeletePoem;\n\n\n// const DeletePoem =(props) =>{\n//     const {onDelete, poem}=props;\n\n//     return ( \n//             // <Alert color=\"danger\">\n//                 <Button color=\"danger\" onClick={()=>onDelete(poem)}>Delete</Button>\n//             // </Alert>\n//          );\n        \n    \n// }\n \n// export default DeletePoem;","/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/cloneOpenMic/openMicClient/simpleOpenMic/src/Components/Poem/ViewAllPoem.js",["127"],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/cloneOpenMic/openMicClient/simpleOpenMic/src/Components/Poem/UpdatePoem.js",[],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/cloneOpenMic/openMicClient/simpleOpenMic/src/Components/Poem/CreatePoem.js",[],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/cloneOpenMic/openMicClient/simpleOpenMic/src/Components/Poem/ViewMyPoem.js",[],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/cloneOpenMic/openMicClient/simpleOp