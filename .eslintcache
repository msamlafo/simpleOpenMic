orization\": localStorage.getItem(\"token\"),\n    //         })\n    //     })\n    //     .then(result => result.json())\n    //     .then(response =>{\n    //         if (response.status === 200){\n    //         console.log(response);\n    //         return <ViewMyProfile />\n    //         }\n    //     })\n    //     .catch(error => console.log(error));\n    // }\n\n    // handleChange =(event) =>{\n    //     const value = event.currentTarget.value;\n    //     const createProfileData = { ...this.state.createProfileData };\n    //     createProfileData[event.currentTarget.name] = value;\n    //     this.setState({createProfileData});\n    // }\n\n    render() { \n        const createProfileData = this.state.createProfileData;\n        return ( \n            <Form onSubmit={(event) =>this.handleSubmit(event)}>\n                <FormGroup row>\n                    <Col sm={2}>\n                        <Label for=\"exampleEmail\" name=\"picUrl\">Picture</Label>\n                    </Col>\n                    <Col sm={6}>\n                        <Input onChange={(event) =>this.handleChange(event)} name=\"picUrl\" type=\"textarea\" placeholder=\"picUrl\" value={createProfileData.picUrl}/>\n                    </Col>\n                </FormGroup>\n                <FormGroup row>\n                <Col sm={2}>\n                    <Label name=\"about\" for=\"exampleEmail\" hidden> About</Label>\n                </Col>\n                <Col sm={6}>\n                    <Input onChange={(event) =>this.handleChange(event)} name=\"about\" type=\"textarea\" placeholder=\"about\" value={createProfileData.about}/>\n                </Col>\n                </FormGroup>\n                <FormGroup row>\n                <Col sm={2}>\n                    <Label name=\"hobbies\" for=\"exampleEmail\" hidden>hobbies</Label>\n                </Col>\n                <Col sm={6}>\n                    <Input onChange={(event) =>this.handleChange(event)} name=\"hobbies\" placeholder=\"Hobbies\" value={createProfileData.hobbies}/>\n                </Col>\n                </FormGroup>\n                <FormGroup row>\n                <Col sm={2}>\n                    <Label name=\"poemWriterSince\" hidden> Writer Since</Label>\n                </Col>\n                <Col sm={6}>\n                    <Input onChange={(event) =>this.handleChange(event)} name=\"poemWriterSince\" placeholder=\"Poem Writer Since\" value={createProfileData.poemWriterSince}/>\n                </Col>\n                </FormGroup>\n                <FormGroup row>\n                <Col sm={2}>\n                    <Label name=\"funFact\" hidden> Fun Fact</Label>\n                </Col>\n                <Col sm={6}>\n                    <Input onChange={(event) =>this.handleChange(event)} name=\"funFact\" placeholder=\"Fun Fact\" value={createProfileData.funFact}/>\n                </Col>\n                </FormGroup>\n                <FormGroup row>\n                <Col sm={2}>\n                    <Label name=\"dreamJob\" hidden> Dream Job</Label>\n                </Col>\n                <Col sm={6}>\n                    <Input onChange={(event) =>this.handleChange(event)} name=\"dreamJob\" placeholder=\"Dream Job\" value={createProfileData.dreamJob}/>\n                </Col>\n                </FormGroup>\n                <FormGroup row>\n                <Col sm={2}>\n                    <Label name=\"resumeUpload\" hidden> Resume Upload</Label>\n                </Col>\n                <Col sm={6}>\n                    <Input onChange={(event) =>this.handleChange(event)} name=\"resumeUpload\" placeholder=\"Resume Upload\" value={createProfileData.resumeUpload}/>\n                </Col>\n                </FormGroup>\n                <FormGroup row>\n                <Col sm={6}>\n                    <label hidden>Submit/Skip Button</label>\n                </Col>\n                {/* <Col sm={4}>\n                </Col> */}\n                <Col sm={3}>\n                    <Button color=\"dark\">Skip</Button>{\" \"}\n                    <Button type=\"submit\" color=\"success\">Submit</Button>\n                </Col>\n                </FormGroup>\n            </Form>\n         \n         );\n    }\n}\n \nexport default CreateProfile;","/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/cloneOpenMic/openMicClient/simpleOpenMic/src/Components/Comment/CreateComment.js",["130"],"import { head } from 'lodash';\nimport React, { Component } from 'react';\nimport { Form } from 'reactstrap';\nimport FormGroupButton from '../../Common/FormGroupButton';\nimport FormInput from '../../Common/FormInput';\n\nclass CreateComment extends Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      createCommentData: {\n        comment: '',\n      },\n    };\n  }\n\n  handleSubmit = (event) => {\n    event.preventDefault();\n    const API_URL = 'http://localhost:4000/comment/create';\n    console.log('submit create comment info', this.state.createCommentData);\n\n    const headers = new Headers();\n    headers.append('Content-Type', 'application/json');\n    headers.append('Authorization', localStorage.getItem('token'));\n    fetch(`${API_URL}`, {\n      method: 'POST',\n      body: JSON.stringify({\n        comment: this.state.createCommentData.comment,\n        poetryId: this.props.poemId,\n      }),\n      headers: headers,\n    })\n      .then((result) => result.json())\n      .then((response) => {\n        if (response.status === 200) {\n          console.log(response);\n        }\n      })\n      .catch((error) => console.log(error));\n  };\n\n  handleChange = (event) => {\n    const value = event.currentTarget.value;\n    const createCommentData = { ...this.state.createCommentData };\n    createCommentData[event.currentTarget.name] = value;\n    this.setState({ createCommentData });\n  };\n\n  render() {\n    const createCommentData = this.state.createCommentData;\n    return (\n      <div className=\"container\">\n        <Form onSubmit={(event) => this.handleSubmit(event)}>\n          <FormInput\n            label=\"Write a comment\"\n            name=\"comment\"\n            id=\"comment\"\n            value={createCommentData.comment}\n            onChange={(event) => this.handleChange(event)}\n          />\n          <FormGroupButton\n            abortButtonLabel=\"Cancel\"\n            affirmButtonLabel=\"Create Comment\"\n            onAbort={this.handleAbort}\n          />\n        </Form>\n      </div>\n    );\n  }\n}\n//taking props from viewpoem and viewcomment\n\nexport default CreateComment;\n","/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/cloneOpenMic/openMicClient/simpleOpenMic/src/Components/Profile/ProfileForm.jsx",[],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/cloneOpenMic/openMicClient/simpleOpenMic/src/Common/FormInput.jsx",[],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/cloneOpenMic/openMicClient/simpleOpenMic/src/Common/FormGroupButton.jsx",[],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/cloneOpenMic/openMicClient/simpleOpenMic/src/Common/ImageInput.jsx",["131"],"import React from 'react';\nimport { FormGroup, Label, Col, Input } from 'reactstrap';\n\nconst ImageInput = (props) => {\n  const { label, name, value, onChange, avUrl } = props;\n\n  const [previewSource, setPreviewSource] = React.useState(avUrl);\n\n  const handleFileInputChange = (e) => {\n    const file = e.target.files[0];\n    onChange(e);\n    previewFile(file);\n  };\n\n  const previewFile = (file) => {\n    const reader = new FileReader();\n    reader.readAsDataURL(file);\n    reader.onloadend = () => {\n      setPreviewSource(reader.result);\n    };\n  };\n\n  const handleSubmit = async (event) => {\n    event.preventDefault();\n    const CLOUD_URL = 'https://api.cloudinary.com/v1_1/dcfmnomqh/image/upload';\n\n    const API_URL_CLOUDSIGN = `${process.env.REACT_APP_API_URL}/profile/cloudsign`;\n\n    const API_URL_IMAGESET = `${process.env.REACT_APP_API_URL}/profile/imageset`;\n\n    const response = await fetch(API_URL_CLOUDSIGN, {\n      method: 'GET',\n      headers: {\n        Authorization: localStorage.getItem('token'),\n      },\n    });\n    const { sig, ts } = await response.json();\n\n    console.log(sig, ts);\n\n    const file = document.getElementById('file-input').files[0];\n\n    console.log(file);\n    const formData = new FormData();\n\n    formData.append('file', file);\n    formData.append('upload_preset', 'openMic');\n    formData.append('api_key', '223952152215851');\n    formData.append('signature', sig);\n    formData.append('timestamp', ts);\n\n    const result = await (\n      await fetch(CLOUD_URL, {\n        method: 'POST',\n        body: formData,\n      })\n    ).json();\n    console.log(result);\n\n    const final = await (\n      await fetch(API_URL_IMAGESET, {\n        method: 'PUT',\n        headers: {\n          Authorization: localStorage.getItem('token'),\n          'Content-Type': 'application/json',\n        },\n        body: JSON.stringify({ url: result.secure_url }),\n      })\n    ).json();\n\n    console.log(final);\n  };\n\n  return (\n    <FormGroup row>\n      <Label for={name} sm={3}>\n        {label}\n      </Label>\n      <Col sm={9}>\n        <Input\n          name={name}\n          id=\"file-input\"\n          placeholder={label}\n          type=\"file\"\n          onChange={(event) => handleFileInputChange(event)}\n        />\n        <img src={previewSource} alt=\"avatar\" height=\"200px\"/>\n        <button\n          type=\"button\"\n          className=\"btn btn-sm btn-secondary m-2\"\n          onClick={(e) => handleSubmit(e)}\n        >\n          Upload Image!\n        </button>\n      </Col>\n    </FormGroup>\n  );\n};\n\nexport default ImageInput;\n","/Users/rdarku/Desktop/Elev